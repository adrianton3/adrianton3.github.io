{
  "globals": {
  },
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-loop-func": 0,
    "eol-last": 0,
    "no-use-before-define": [2, "nofunc"],
    "new-cap": 0,
    "no-constant-condition": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
